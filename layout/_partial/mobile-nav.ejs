<nav id="mobile-nav">
  <div>
    <% for (var i in theme.menu){ %>
      <a href="<%- url_for(theme.menu[i]) %>" class="mobile-nav-link"><%= i %></a>
    <% } %>
  </div>
  <div id="mobile-nav-social">
    <%- partial('header-icon') %>
    <% if (theme.rss){ %>
      <a id="nav-rss-link" class="nav-icon fa-rss" href="<%- theme.rss %>" title="RSS Feed"></a>
    <% } %>
  </div>
</nav>
